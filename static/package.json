{
  "name": "kyoto",
  "version": "0.0.0",
  "description": "",
  "main": "tailwind.config.js",
  "scripts": {
    "build": "npm run build:css && npm run build:img",
    "build:css": "tailwindcss -m -c tailwind.config.js -o dist/css/tailwind.min.css",
    "build:css:w": "tailwindcss -w -c tailwind.config.js -o dist/css/tailwind.min.css",
    "build:img": "(rm -rf dist/img && mkdir -p dist/img && cp -r img/* dist/img/) || true"
  },
  "author": "Yurii Zinets",
  "license": "MIT",
  "dependencies": {
    "tailwindcss": "^3.0.23"
  }
}
